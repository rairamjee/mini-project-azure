{
	"name": "sc_report_DF",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sc_categories_list_prep_sql",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "sc_product_list_prep_sql",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sc_subcategories_categories_count_report_sql",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "sc_products_delivery_gap_report_sql",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "window1"
				},
				{
					"name": "select1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "select2"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Category as string,",
				"          CategoryId as string,",
				"          Subcategory as string,",
				"          SubcategoryName as string,",
				"          SubcategoryId as string,",
				"          SeriesCode as string,",
				"          SeriesName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          SeriesName as string,",
				"          SeriesCode as string,",
				"          SubcategoryId as string,",
				"          DepartmentCode as string,",
				"          BrandName as string,",
				"          BrandCode as string,",
				"          MinShipDays as integer,",
				"          MaxShipDays as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1 window(over(CategoryId),",
				"     desc(SubcategoryId, true),",
				"     SubcategorgyCount = rowNumber()) ~> window1",
				"window1 select(mapColumn(",
				"          Category,",
				"          CategoryId,",
				"          Subcategory,",
				"          SubcategoryName,",
				"          SubcategoryId,",
				"          SeriesCode,",
				"          SeriesName,",
				"          SubcategorgyprodCount = SubcategorgyCount",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"source2, source1 lookup(source2@SubcategoryId == source1@SubcategoryId,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 select(mapColumn(",
				"          SeriesName = source2@SeriesName,",
				"          SeriesCode = source2@SeriesCode,",
				"          SubcategoryId = source2@SubcategoryId,",
				"          DepartmentCode,",
				"          BrandName,",
				"          BrandCode,",
				"          MinShipDays,",
				"          MaxShipDays,",
				"          Category,",
				"          CategoryId,",
				"          Subcategory,",
				"          SubcategoryName,",
				"          SubcategoryId = source1@SubcategoryId,",
				"          SeriesCode = source1@SeriesCode,",
				"          SeriesName = source1@SeriesName",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 derive(DaysWait = MaxShipDays-MinShipDays) ~> derivedColumn1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink2"
			]
		}
	}
}